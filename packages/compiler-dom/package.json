{
  "name": "@zeus-js/compiler",
  "version": "0.0.1",
  "description": "JSX转换、代码优化和类型检查",
  "main": "index.js",
  "module": "dist/compiler.esm-bundler.js",
  "types": "dist/compiler.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/compiler.d.ts",
      "node": {
        "production": "./dist/compiler.cjs.prod.js",
        "development": "./dist/compiler.cjs.js",
        "default": "./index.js"
      },
      "module": "./dist/compiler.esm-bundler.js",
      "import": "./dist/compiler.esm-bundler.js",
      "require": "./index.js"
    },
    "./*": "./*"
  },
  "buildOptions": {
    "formats": [
      "esm",
      "cjs"
    ]
  },
  "dependencies": {
    "@zeus-js/compiler-core": "workspace:*"
  },
  "peerDependencies": {
    "@babel/core": "^7.26.5"
  },
  "devDependencies": {
    "@babel/types": "catalog:",
    "@types/babel__core": "^7.20.5"
  }
}
