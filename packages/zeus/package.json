{
  "name": "zeus-js",
  "version": "0.0.1",
  "description": "轻量级响应式前端框架",
  "main": "index.js",
  "module": "dist/zeus.esm-bundler.js",
  "types": "dist/zeus.d.ts",
  "files": [
    "index.js",
    "index.mjs",
    "dist",
    "server-renderer",
    "jsx-runtime",
    "jsx.d.ts"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/zeus.d.ts",
        "node": "./index.mjs",
        "default": "./dist/zeus.runtime.esm-bundler.js"
      },
      "require": {
        "types": "./dist/zeus.d.ts",
        "node": {
          "production": "./dist/zeus.cjs.prod.js",
          "development": "./dist/zeus.cjs.js",
          "default": "./index.js"
        },
        "default": "./index.js"
      }
    },
    "./server-renderer": {
      "import": {
        "types": "./server-renderer/index.d.mts",
        "default": "./server-renderer/index.mjs"
      },
      "require": {
        "types": "./server-renderer/index.d.ts",
        "default": "./server-renderer/index.js"
      }
    },
    "./jsx-runtime": {
      "types": "./jsx-runtime/index.d.ts",
      "import": "./jsx-runtime/index.mjs",
      "require": "./jsx-runtime/index.js"
    },
    "./jsx-dev-runtime": {
      "types": "./jsx-runtime/index.d.ts",
      "import": "./jsx-runtime/index.mjs",
      "require": "./jsx-runtime/index.js"
    },
    "./jsx": "./jsx.d.ts",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "buildOptions": {
    "name": "Zeus",
    "formats": [
      "esm",
      "cjs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/zeus.git"
  },
  "keywords": [
    "framework",
    "frontend",
    "reactive",
    "zeus",
    "javascript",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/username/zeus/issues"
  },
  "homepage": "https://github.com/username/zeus#readme",
  "dependencies": {
    "@zeus-js/reactivity": "workspace:*",
    "@zeus-js/runtime-core": "workspace:*",
    "@zeus-js/runtime-dom": "workspace:*"
  }
}
