{
  "name": "@zeus/compiler",
  "version": "0.0.1",
  "description": "JSX转换、代码优化和类型检查",
  "main": "index.js",
  "module": "dist/compiler.esm-bundler.js",
  "types": "dist/compiler.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/compiler.d.ts",
      "node": {
        "production": "./dist/compiler.cjs.prod.js",
        "development": "./dist/compiler.cjs.js",
        "default": "./index.js"
      },
      "module": "./dist/compiler.esm-bundler.js",
      "import": "./dist/compiler.esm-bundler.js",
      "require": "./index.js"
    },
    "./*": "./*"
  },
  "buildOptions": {
    "formats": [
      "esm",
      "cjs"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-syntax-decorators": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/template": "^7.26.9",
    "@zeus/shared": "workspace:*",
    "babel-plugin-jsx-dom-expressions": "^0.39.7"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5"
  }
}
