{
  "name": "@zeus/output",
  "version": "0.0.1",
  "description": "生成不同框架的组件绑定",
  "main": "index.js",
  "module": "dist/output.esm-bundler.js",
  "types": "dist/output.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/output.d.ts",
      "node": {
        "production": "./dist/output.cjs.prod.js",
        "development": "./dist/output.cjs.js",
        "default": "./index.js"
      },
      "module": "./dist/output.esm-bundler.js",
      "import": "./dist/output.esm-bundler.js",
      "require": "./index.js"
    },
    "./*": "./*"
  },
  "buildOptions": {
    "formats": [
      "esm",
      "cjs"
    ]
  },
  "dependencies": {
    "@zeus/shared": "workspace:*"
  },
  "peerDependencies": {
    "rollup": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "~5.7.2"
  }
}
